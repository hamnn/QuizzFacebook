{% extends "MetinetFacebookBundle::base.html.twig" %}
{% block title %}Créer un nouveau thème{% endblock %}
{% block stylesheets %}
        {{ parent() }}
        <link rel="stylesheet" href="{{ asset('/bundles/metinetfacebook/css/admin.css') }}" type="text/css" />
{% endblock %}
{% block body %}
<div id='header'>
    <a href="{{ path('admin_index') }}"><h1 id="logo">YouQuizz-administration</h1></a>
</div>
<div id="page">
    <div id="contenu"> 
<h1>Nouveau thème</h1>

<form action="{{ path('theme_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

        <button  class="deleteBouton" type="submit">Créer</button>

        <a  class="lienBouton" href="{{ path('theme') }}">
            Retour à la liste
        </a>

</form>


    </div>
    </div>
{% endblock %}